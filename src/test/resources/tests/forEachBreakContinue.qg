init {
    out = 0
}

run {
    arr = arrayList(0, 1, 2, 3, 4)
    for (i in arr) {
        if (i == 3) {
            break
        }
        out += pow(2, i) // 1 + 2 + 4 = 7
    }
    for (i in arr) {
        if (i == 3) {
            continue
        }
        out += pow(2, i) // 7 + 1 + 2 + 4 + 16 = 30
    }
    for (;true;) @aLabel {
        while(true) @otherLabel {
            for (i in arr) {
                out++ // 30 + 1 = 31
                break @aLabel
            }
        }
    }
}

