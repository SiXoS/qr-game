fun sum(toSum: vararg<number>): number {
    sum = 0
    for (elem in toSum) {
        sum += elem
    }
    return sum
}

fun sumWithBase(base: number, toSum: vararg<number>): number {
    sum = base
    for (elem in toSum) {
        sum += elem
    }
    return sum
}